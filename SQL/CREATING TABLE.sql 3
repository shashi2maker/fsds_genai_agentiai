CREATE TABLE employee(
    employee_id SERIAL PRIMARY KEY,
	name VARCHAR(100) NOT NULL,
	position VARCHAR (50),
	deparment VARCHAR(50),
	hire_date DATE,
	salary NUMERIC(10,2)
	
);



SELECT *FROM employee;

